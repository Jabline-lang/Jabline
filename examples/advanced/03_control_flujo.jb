// Ejemplo 10: Control de Flujo (break/continue)

// Bucle con continue (saltar iteración)
echo("Números impares del 1 al 6:");
for (let i = 1; i <= 6; i++) {
    if (i % 2 == 0) {
        continue;  // Saltar números pares
    }
    echo("Impar: " + i);
}

// Bucle con break (salir del bucle)
echo("Buscar el número 5:");
for (let j = 1; j <= 10; j++) {
    if (j == 5) {
        echo("¡Encontrado el 5!");
        break;  // Salir del bucle
    }
    echo("Buscando... " + j);
}
